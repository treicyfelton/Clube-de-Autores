#background
  %h2 Todas as histórias
  %table.listing
    %thead
      %td.list Título
      %td.list Autor
      %td Deletar?
    %tbody  
      - @histories.each do |history|
        %tr
          %td.t= link_to history.title, history, class:"colored"
          %td.t= link_to User.find(history.user_id).name, "/users/#{history.user_id}", class:"colored"
          %td.t= button_to 'Deletar', "/admin/histories/#{history.id}", method: :delete